<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.Advertiser.Campaign.NewCampaign.ChoseVid"
             BackgroundColor="#FAFAFA"
             xmlns:templates="clr-namespace:App1.Templates">
    <ContentPage.Content>
        <ScrollView>
            <StackLayout x:Name="videos" Padding="0" Margin="0">
                <Frame x:Name="videoLoading" IsVisible="true" VerticalOptions="Center" HorizontalOptions="Center" BackgroundColor="Transparent">
                    <ActivityIndicator x:Name="actInd" IsRunning="True" AbsoluteLayout.LayoutFlags="PositionProportional" AbsoluteLayout.LayoutBounds="0.5,0.5,-1,-1" Grid.Row="0" />
                </Frame>
            </StackLayout>
        </ScrollView>
    </ContentPage.Content>
</ContentPage>